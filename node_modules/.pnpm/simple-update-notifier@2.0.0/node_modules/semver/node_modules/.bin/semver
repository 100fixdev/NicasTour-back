#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/admin/Desktop/Hackathon-2025/Nicastours/nicastour-back/node_modules/.pnpm/semver@7.7.2/node_modules/semver/bin/node_modules:/proc/cygdrive/c/Users/admin/Desktop/Hackathon-2025/Nicastours/nicastour-back/node_modules/.pnpm/semver@7.7.2/node_modules/semver/node_modules:/proc/cygdrive/c/Users/admin/Desktop/Hackathon-2025/Nicastours/nicastour-back/node_modules/.pnpm/semver@7.7.2/node_modules:/proc/cygdrive/c/Users/admin/Desktop/Hackathon-2025/Nicastours/nicastour-back/node_modules/.pnpm/node_modules:/proc/cygdrive/c/Users/admin/Desktop/Hackathon-2025/Nicastours/nicastour-back/node_modules:/proc/cygdrive/c/Users/admin/Desktop/Hackathon-2025/Nicastours/node_modules:/proc/cygdrive/c/Users/admin/Desktop/Hackathon-2025/node_modules:/proc/cygdrive/c/Users/admin/Desktop/node_modules:/proc/cygdrive/c/Users/admin/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/admin/desktop/Hackathon-2025/Nicastours/nicastour-back/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/admin/Desktop/Hackathon-2025/Nicastours/nicastour-back/node_modules/.pnpm/semver@7.7.2/node_modules/semver/bin/node_modules:/proc/cygdrive/c/Users/admin/Desktop/Hackathon-2025/Nicastours/nicastour-back/node_modules/.pnpm/semver@7.7.2/node_modules/semver/node_modules:/proc/cygdrive/c/Users/admin/Desktop/Hackathon-2025/Nicastours/nicastour-back/node_modules/.pnpm/semver@7.7.2/node_modules:/proc/cygdrive/c/Users/admin/Desktop/Hackathon-2025/Nicastours/nicastour-back/node_modules/.pnpm/node_modules:/proc/cygdrive/c/Users/admin/Desktop/Hackathon-2025/Nicastours/nicastour-back/node_modules:/proc/cygdrive/c/Users/admin/Desktop/Hackathon-2025/Nicastours/node_modules:/proc/cygdrive/c/Users/admin/Desktop/Hackathon-2025/node_modules:/proc/cygdrive/c/Users/admin/Desktop/node_modules:/proc/cygdrive/c/Users/admin/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/admin/desktop/Hackathon-2025/Nicastours/nicastour-back/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/semver.js" "$@"
else
  exec node  "$basedir/../../bin/semver.js" "$@"
fi
